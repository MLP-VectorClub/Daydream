{
  "name": "mlpvectorclub",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run write-git-info && ng serve --host $HOST --proxy-config development.proxy.json --ssl true --ssl-cert \"$SSL_CERT\" --ssl-key \"$SSL_KEY\"",
    "build": "npm run write-git-info && ng build",
    "gen-api-types": "node generate-types.js",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "run-s build:client-and-server-bundles compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run mlpvectorclub:server:production",
    "start:ssr": "run-s build:ssr serve:ssr",
    "write-git-info": "node src/environments/version.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/platform-server": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@fortawesome/angular-fontawesome": "^0.4.0",
    "@fortawesome/fontawesome-free": "^5.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@ngrx/effects": "^7.4.0",
    "@ngrx/entity": "^7.4.0",
    "@ngrx/store": "^7.4.0",
    "@ngrx/store-devtools": "^7.4.0",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "0.0.0",
    "@ngx-translate/core": "^11.0.1",
    "build-url": "^1.3.3",
    "capitalize": "^2.0.0",
    "core-js": "^2.5.4",
    "date-fns": "^2.0.0-alpha.27",
    "express": "^4.15.2",
    "hammerjs": "^2.0.8",
    "ng-zorro-antd": "^7.4.1",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@ngrx/schematics": "^7.4.0",
    "@types/capitalize": "^1.0.1",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.16.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.9.5",
    "codelyzer": "~4.5.0",
    "dotenv": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "openapi-to-typescript": "^3.2.0",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "~3.2.2",
    "webpack-cli": "^3.1.0"
  }
}
