{
  "name": "daydream",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "npm run write-git-info && ng serve --host $HOST --proxy-config development.proxy.json --ssl true --ssl-cert \"$SSL_CERT\" --ssl-key \"$SSL_KEY\"",
    "build": "npm run write-git-info && ng build --prod",
    "api:types": "node generate-types.js",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "run-s build:client-and-server-bundles compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run mlpvectorclub:server:production",
    "start:ssr": "run-s build:ssr serve:ssr",
    "write-git-info": "node src/environments/version.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.2",
    "@angular/cdk": "^8.1.3",
    "@angular/common": "^8.1.3",
    "@angular/compiler": "^8.1.3",
    "@angular/core": "^8.1.3",
    "@angular/forms": "^8.1.3",
    "@angular/platform-browser": "^8.1.3",
    "@angular/platform-browser-dynamic": "^8.1.3",
    "@angular/platform-server": "^8.1.3",
    "@angular/router": "^8.1.3",
    "@fortawesome/fontawesome-free": "^5.10.1",
    "@ngrx/effects": "^8.2.0",
    "@ngrx/entity": "^8.2.0",
    "@ngrx/store": "^8.2.0",
    "@ngrx/store-devtools": "^8.2.0",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@ngx-translate/core": "^11.0.1",
    "@types/http-proxy": "^1.17.0",
    "build-url": "^1.3.3",
    "capitalize": "^2.0.0",
    "core-js": "^3.2.1",
    "date-fns": "^2.0.0-beta.5",
    "express": "^4.17.1",
    "express-php-fpm": "^2.0.2",
    "hammerjs": "^2.0.8",
    "http-proxy": "^1.17.0",
    "intersection-observer": "^0.7.0",
    "lodash-es": "^4.17.15",
    "ng-in-viewport": "^6.0.3",
    "ng-zorro-antd": "^8.2.0",
    "ngx-progressive-image-loader": "^3.0.2",
    "rxjs": "~6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
